{
  "name": "vue-ecommerce",
  "ProductName": "vue-ecommerce",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main",
  "author": "Rahul Kandwal",
  "scripts": {
    "dev": "npm-run-all --parallel dev:vue dev:electron",
    "dev:vue": "vite",
    "dev:public:vue":" vite --host 0.0.0.0 --port 5123",
    "dev:electron": "npm run transplie:electron && cross-env NODE_ENV=development electron .",
    "build:vue": "vue-tsc -b && vite build",
    "preview:vue": "vite preview",
    "transplie:electron": "tsc --project src/electron/tsconfig.json",
    "build:mac": "npm run transplie:electron && npm run build:vue && electron-builder --mac --arm64",
    "build:win": "npm run transplie:electron && npm run build:vue && electron-builder --win --x64",
    "build:linux": "npm run transplie:electron && npm run build:vue && electron-builder --linux --x64"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@mdi/font": "^7.4.47",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/vue-table": "^8.21.3",
    "@vee-validate/zod": "^4.15.1",
    "os-utils": "^0.0.14",
    "pinia": "^3.0.3",
    "tailwindcss": "^4.1.11",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.17",
    "vue-hook-form": "^0.0.4",
    "vue-remix-icons": "^3.15.0",
    "vue-router": "^4.5.1",
    "vue3-toastify": "^0.2.8",
    "vuetify": "^3.9.4",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/os-utils": "^0.0.4",
    "@types/vue-router": "^2.0.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^10.0.0",
    "electron": "^37.2.5",
    "electron-builder": "^26.0.12",
    "npm-run-all": "^4.1.5",
    "sass": "^1.90.0",
    "sass-loader": "^16.0.5",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vue-tsc": "^2.2.12"
  }
}
